<!DOCTYPE html>
<html lang="en">

 <%- include('includes/head') %>

<body>
    <!-- Begin page -->
    <div class="wrapper">

        
       

           <%- include('includes/sidebar') %>
               

        <style>
            body[data-sidebar-size="condensed"] .side-nav .collapse {
    display: none !important;
}
        </style>
        

        <%- include('includes/topbar') %>

    

        <!-- Search Modal -->
        <div class="modal fade" id="searchModal" tabindex="-1" aria-labelledby="searchModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-lg">
                <div class="modal-content bg-transparent">
                    <form>
                        <div class="card mb-1">
                            <div class="px-3 py-2 d-flex flex-row align-items-center" id="top-search">
                                <i class="ti ti-search fs-22"></i>
                                <input type="search" class="form-control border-0" id="search-modal-input" placeholder="Search for actions, people,">
                                <button type="submit" class="btn p-0" data-bs-dismiss="modal" aria-label="Close">[esc]</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>

        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->
        <div class="page-content">
            <div class="page-container">

                
                <div class="page-title-head d-flex align-items-center gap-2">
                    <div class="flex-grow-1">
                        <h4 class="fs-17 mb-0">Ana Sayfa</h4>
                    </div>

                    <div class="text-end">
                        <ol class="breadcrumb m-0 py-0 fs-13">
                            <li class="breadcrumb-item"><a href="javascript: void(0);"><%= name %></a></li>
                            
                            <li class="breadcrumb-item active">Atina</li>
                        </ol>
                    </div>
                </div>
                

                

                <div class="row row-cols-xxl-4 row-cols-md-2 row-cols-1">
    
                    <!-- Total Visitors -->
                    <div class="col">
                        <div class="card">
                        <div class="card-body">
                                <h5 class="text-muted fs-13 text-uppercase" title="Total Visitors">Kullanıcı Sayısı</h5>
                                <div class="d-flex align-items-center gap-2 my-3">
                                    <h3 class="mb-0"></span><%= totalUsers %></h3>
                                    <i data-lucide="users" class="ms-auto display-2 position-absolute opacity-25 text-muted widget-icon"></i>
                                </div>
                                <p class="mb-0 text-muted">
                                    
                                    <span class="text-nowrap">Atina</span>
                                </p>
                            </div>
                        </div>
                    </div>
                
                    <!-- Unique Visitors -->
                    <div class="col">
                        <div class="card">
                            <div class="card-body">
                                <h5 class="text-muted fs-13 text-uppercase" title="Unique Visitors">Üyelik Türü</h5>
                                <div class="d-flex align-items-center gap-2 my-3">
                                    <h3 class="mb-0"></span><%= roleName %></h3>
                                    <i class="ti ti-user-hexagon ms-auto display-1 position-absolute opacity-25 text-muted widget-icon"></i>
                                </div>
                                <p class="mb-0 text-muted">
                                    
                                    <span class="text-nowrap">Atina</span>
                                </p>
                            </div>
                        </div>
                    </div>
                <link rel="stylesheet" href="https://storage.perfectcdn.com/css/font-awesome/css/all.min.css" />

                    <!-- Page Views -->
                    <div class="col">
                        <div class="card">
                            <div class="card-body">
                                <h5 class="text-muted fs-13 text-uppercase" title="Page Views">Çevrimiçi Kullanıcılar</h5>
                                <div class="d-flex align-items-center gap-2 my-3">
                                    <h3 class="mb-0"><span><%= onlineCount %></span></h3>
                                    <i data-lucide="globe" class="fa-globle display-1 position-absolute opacity-25 text-muted widget-icon"></i>
                                </div>
                                <p class="mb-0 text-muted">
                                    <% if (onlineUsers && onlineUsers.length > 0) { %>
                                      <span class="text-nowrap">Atina 
                                        <% onlineUsers.forEach(u => { %>
                                          
                                        <% }) %>
                                      </span>
                                    <% } else { %>
                                      <span class="text-nowrap">Hiç kimse çevrimiçi değil</span>
                                    <% } %>
                                </p>
                            </div>
                        </div>
                    </div>
                
                    <!-- Bounce Rate -->
                    <div class="col">
                        <div class="card">
                            <div class="card-body">
                                <h5 class="text-muted fs-13 text-uppercase" title="Bounce Rate">Üyelik Süresi</h5>
                                <div class="d-flex align-items-center gap-2 my-3">
                                    <h3 class="mb-0"><span ><%= membershipText %></span></h3>
                                    <i class="ti ti-clock ms-auto display-1 position-absolute opacity-25 text-muted widget-icon"></i>
                                </div>
                                <p class="mb-0 text-muted">
                                   <% if (membershipExpiresAt) { %>
                                     <span class="text-nowrap">Bitiş: <%= new Date(membershipExpiresAt).toLocaleDateString('tr-TR') %></span>
                                   <% } else { %>
                                     <span class="text-nowrap">Atina</span>
                                   <% } %>
                                </p>
                            </div>
                        </div>
                    </div>
                
                </div><!-- end row -->
                




                <div class="row">
                    <div class="col-xxl-12">
                        <div class="card">
                            <div class="d-flex card-header justify-content-between align-items-center">
                                <h4 class="card-title">Son Giriş Yapanlar</h4>
                                
                            </div>
                            <div class="card-body p-0">
                                <div class="bg-light bg-opacity-50 py-1 text-center">
                                    <p class="m-0"><b></b> En son oturum açan üyeleri gösterir <span class="fw-medium"></span></p>
                                </div>
                                <div class="table-responsive">
                                    <table class="table table-custom table-centered table-sm table-nowrap table-hover mb-0">
  <thead>
    <tr>
      <th>Kullanıcı</th>
      <th>Son Giriş</th>
      <th>Durumu</th>
    </tr>
  </thead>
  <tbody>
    <% if (recentLogins && recentLogins.length > 0) { %>
      <% recentLogins.forEach(user => { %>
        <tr>
          <td>
            <div class="d-flex align-items-center">
              <div class="avatar-md flex-shrink-0 me-2">
                <% if(user.avatar) { %>
                  <img src="<%= user.avatar %>" alt="<%= user.name %>" class="rounded-circle" width="30" height="30" />
                <% } else { %>
                  <span class="avatar-title text-bg-white  rounded-circle fw-semibold" style="width:35px; height:35px; display:flex; align-items:center; justify-content:center;">
                    <i class="fa fa-user"></i>
                  </span>
                <% } %>
              </div>
              <div>
                <h5 class="fs-14 mb-0"><%= user.name %></h5>
              </div>
            </div>
          </td>
          <td>
            <% if(user.lastLoginAt) { %>
              <small class="text-muted"><%= timeAgo(user.lastLoginAt) %></small>
            <% } else { %>
              <small class="text-muted">Bilinmiyor</small>
            <% } %>
          </td>
          <td>
            <i class="fa fa-circle fs-12 <%= (onlineUsers||[]).some(o => o.email === user.email) ? 'text-success' : 'text-secondary' %>"></i>
            <%= (onlineUsers||[]).some(o => o.email === user.email) ? 'Çevrimiçi' : 'Çevrimdışı' %>
          </td>
        </tr>
      <% }); %>
    <% } else { %>
      <tr>
        <td colspan="3" class="text-center">Henüz oturum açan yok.</td>
      </tr>
    <% } %>
  </tbody>
</table>

                                </div> <!-- end table-responsive-->
                            </div> <!-- end card-body-->

                            

                        </div> <!-- end card-->
                    </div> <!-- end col-->

                  <div class="col-xxl-12">
    <div class="card card-h-100">
        <div class="card-header d-flex flex-wrap align-items-center gap-2">
            <h4 class="card-title me-auto">Duyurular</h4>
            <div class="d-flex gap-2 justify-content-end text-end">
                <!-- Filtre veya yeni duyuru butonu eklenebilir -->
            </div>
        </div>
        <div class="card-body p-0">
            <div class="table-responsive">
                <table class="table table-custom table-centered table-sm table-nowrap table-hover mb-0">
                    <thead>
                        <tr>
                            <th>Başlık</th>
                            <th>Açıklama</th>
                            <th>Tarih</th>
                            <th>Durum</th>
                          
                        </tr>
                    </thead>
                    <tbody>
                        <% if (duyurular && duyurular.length > 0) { %>
                            <% duyurular.forEach(d => { %>
                                                                <% 
                                                                    let bgClass = 'bg-primary-subtle', iconClass = 'text-primary', iconName = 'ti ti-bell';
                                                                    if (d.type === 'Bilgi') { bgClass = 'bg-info-subtle'; iconClass = 'text-info'; iconName = 'ti ti-info-circle'; }
                                                                    else if (d.type === 'Uyarı') { bgClass = 'bg-warning-subtle'; iconClass = 'text-warning'; iconName = 'ti ti-alert-triangle'; }
                                                                    else if (d.type === 'Başarı') { bgClass = 'bg-success-subtle'; iconClass = 'text-success'; iconName = 'ti ti-check'; }
                                                                    else if (d.type === 'Hata') { bgClass = 'bg-danger-subtle'; iconClass = 'text-danger'; iconName = 'ti ti-x'; }
                                                                %>
                                <tr>
                                    <td>
                                        <div class="d-flex align-items-center">
                                            <div class="avatar-md flex-shrink-0 me-2">
                                                    <span class="avatar-title <%= bgClass %> rounded-circle">
                                                        <i class="<%= iconName %> fs-18 <%= iconClass %>"></i>
                                                    </span>
                                            </div>
                                            <div>
                                                <h5 class="fs-16 mb-0"><%= d.title || 'Başlık Yok' %></h5>
                                            </div>
                                        </div>
                                    </td>
                                    <td><span class="text-muted fs-14"><%= d.text %></span></td>
                                    <td><span class="text-muted fs-12"><%= d.date ? new Date(d.date).toLocaleDateString('tr-TR') : '' %></span></td>
                                    <td>
                                        <h5 class="fs-14 mb-0 fw-normal">
                                            <i class="ti ti-circle-filled fs-12 <%= d.active ? 'text-success' : 'text-secondary' %>"></i> <%= d.active ? 'Aktif' : 'Pasif' %>
                                        </h5>
                                    </td>
                                   
                                </tr>
                            <% }) %>
                        <% } else { %>
                            <tr>
                                <td colspan="5" class="text-center">Henüz duyuru yok.</td>
                            </tr>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

                            
                            </div>
                        </div> <!-- end card-->
                    </div> <!-- end col-->
                </div> <!-- end row-->

            </div> <!-- container -->

        <%- include('includes/footer') %>

            

        </div>

        <!-- ============================================================== -->
        <!-- End Page content -->
        <!-- ============================================================== -->

    </div>
    <!-- END wrapper -->

