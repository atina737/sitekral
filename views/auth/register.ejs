         <script>
            document.getElementById('togglePassword').addEventListener('click', function () {
  const passwordInput = document.getElementById('passwordInput');
  const icon = this;

  if (passwordInput.type === 'password') {
    passwordInput.type = 'text';
    icon.classList.remove('fa-eye-slash');
    icon.classList.add('fa-eye');
  } else {
    passwordInput.type = 'password';
    icon.classList.remove('fa-eye');
    icon.classList.add('fa-eye-slash');
  }
});
         </script>
    
 
 <script>
    (function(){
        const f=document.getElementById('registerForm'),E=document.getElementById('regError');
        
        // Bot tespiti için event tracking
        let mouseEvents = 0;
        let keyboardEvents = 0;
        
        document.addEventListener('mousemove', () => mouseEvents++);
        document.addEventListener('keydown', () => keyboardEvents++);
        f.addEventListener('submit',async function(e){
            e.preventDefault();
            
            // Event sayılarını güncelle
            document.getElementById('mouseEvents').value = mouseEvents;
            document.getElementById('keyboardEvents').value = keyboardEvents;
            
            E.className='alert d-none mt-2';
            E.textContent='';
            const name=f.name.value.trim(),email=f.email.value.trim(),password=f.password.value;
            if(!name||!email||!password){
                E.textContent='Lütfen tüm alanları doldurun.';
                E.classList.remove('d-none');
                E.classList.add('alert-danger');
                return;
            }
            try{
                const res=await fetch('/api/register',{
                    method:'POST',
                    credentials:'same-origin',
                    headers:{
                        'Content-Type':'application/json',
                        'X-CSRF-Token':(document.querySelector('meta[name="csrf-token"]')?.getAttribute('content')||'')
                    },
                    body:JSON.stringify({name,email,password})
                });
                const data=await res.json().catch(()=>({}));
                if(res.ok&&data.ok){
                   
                    E.textContent=data.message||'Kayıt başarılı. Yönlendiriliyorsunuz...';
                    E.classList.remove('d-none');
                    E.classList.add('alert-success');
                    setTimeout(()=>window.location.href=data.redirect||'/login',3000);
                }else{
                    E.textContent=(data&&data.message)||'Kayıt başarısız.';
                    E.classList.remove('d-none');
                    E.classList.add('alert-danger');
                }
            }catch{
                E.textContent='Ağ hatası. Lütfen tekrar deneyin.';
                E.classList.remove('d-none');
                E.classList.add('alert-danger');
            }
        });
    })();
</script>

             
 
    


